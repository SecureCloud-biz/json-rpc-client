{
  "name": "strider2038/json-rpc-client",
  "description": "Tiny JSON RPC v2 client for PHP",
  "keywords": [
    "jsonrpc",
    "jsonrpc2",
    "client",
    "http",
    "tcp"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Lazarev",
      "email": "strider2038@yandex.ru"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phake/phake": "^3.1",
    "ramsey/uuid": "^3.8"
  },
  "suggest": {
    "ramsey/uuid": "Library for generating UUID in request identifiers"
  },
  "autoload": {
    "psr-4": {
      "Strider2038\\JsonRpcClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Strider2038\\JsonRpcClient\\Tests\\": "tests/"
    }
  }
}
